{
  "extends": ["tslint:latest"],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "interface-name": true,
    "class-name": true,
    "no-duplicate-imports": true,
    "no-empty-interface": true,
    "quotemark": [false],
    "ordered-imports": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
